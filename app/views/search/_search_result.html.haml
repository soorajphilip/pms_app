- if parking_history.present?
  .table-responsive
    %table.table.table-striped.table-sm.table-hover.table-bordered
      %thead
        %tr
          %th{:scope => "col"} #
          %th{:scope => "col"} Slot
          %th{:scope => "col"} Section
          %th{:scope => "col"} Date
          %th{:scope => "col"} Entry
          %th{:scope => "col"} Exit
          %th{:scope => "col"} Total Time Parked
      %tbody
        - parking_history.each.with_index(1) do |ph,i|
          %tr
            %td= i
            %td= ph.slot
            %td= ph.section
            %td= ph.date.strftime("%a, %d-%m-%Y")
            %td= ph.entry_time.strftime("%I:%M %p")
            %td Nil
            %td Nil
- else
  %h6 No Data Available
